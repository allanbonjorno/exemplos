programa
{
	
	funcao inicio()
	{
		inteiro vetor[10] = {1,3,4,5,2,6,10,8,7,6}, a, b, vaux
		
		para(a=0; a<10; a++)
		{
			para(b=a+1; b<10; b++)
			{
				se(vetor[b] > vetor[a]) 
				{  
					vaux = vetor[b]
					vetor[b] = vetor[a]
					vetor[a] = vaux
				}
			}
		}
		
		escreva("Sequencia em ordem decrescente: \n")
		
		para (a=0; a<10; a++)
		{
			escreva(vetor[a], " ")
		}
		escreva("\n")
	}
}
